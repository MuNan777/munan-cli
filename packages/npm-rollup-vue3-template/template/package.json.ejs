{
  "name": "<%=packageName %>",
  "version": "0.0.1",
  "description": "<%=description %>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run lint && npm run build:clean && npm run build:esm && npm run build:cjs && npm run build:umd",
    "lint": "vue-cli-service lint",
    "build:esm": "npx rollup -c build/rollup.config.esm.js",
    "build:cjs": "npx rollup -c build/rollup.config.cjs.js",
    "build:umd": "npx rollup -c build/rollup.config.umd.js",
    "build:clean": "rimraf ./dist",
    "deploy": "npm run build && npm publish"
  },
  "main": "dist/components-lib.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/components-lib.mjs",
      "require": "./dist/components-lib.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.19",
    "@vue/cli-plugin-eslint": "~4.5.19",
    "@vue/cli-plugin-typescript": "~4.5.19",
    "@vue/cli-service": "~4.5.19",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-typescript2": "^0.32.1",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "~4.1.5"
  }
}
