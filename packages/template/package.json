{
  "name": "@munan-cli/template",
  "version": "1.0.2",
  "description": "munan-cli 模板构建模块",
  "main": "./lib/index.mjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "*.d.ts",
    "*.mjs"
  ],
  "engines": {
    "node": ">=v14.16.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest __test__/",
    "build": "node ./build.js && rimraf lib && rollup -c",
    "dev": "rollup -c --watch --watch.include=src/** -m inline"
  },
  "dependencies": {
    "@munan-cli/utils": "^1.0.3",
    "fs-extra": "^10.1.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MuNan777/munan-cli.git"
  },
  "bugs": {
    "url": "https://github.com/MuNan777/munan-cli/issues"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13"
  },
  "templateConfigs": [
    {
      "moduleName": "@munan-cli/vite-vue3-ts-template",
      "templateName": "vite-vue3-ts 模板",
      "templateType": "executable",
      "templateTag": "project",
      "startCommand": "npm run dev",
      "buildPath": "dist",
      "ejsIgnoreFiles": [
        "**/node_modules/**"
      ]
    },
    {
      "moduleName": "@munan-cli/webpack-vue2-standard-template",
      "templateName": "webpack-vue2-standard 模板",
      "templateType": "general",
      "templateTag": "project",
      "startCommand": "npm run serve",
      "buildPath": "dist",
      "ejsIgnoreFiles": [
        "**/node_modules/**",
        "**/public/**"
      ]
    }
  ]
}
